pipeline {
    agent any
    environment {
        RELEASE='20.04'
    }

    stages {
        stage('Build') {
            agent any
            environment {
                LOG_LEVEL='INFO'
            }
            steps {
                echo "Building ${RELEASE} with log level ${LOG_LEVEL}"
            }
        }
        stage('test') {
            steps {
                echo "Testing. I can release ${RELEASE} but not log level ${LOG_LEVEL}"
            }
        }
    }
}

